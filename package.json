{
  "name": "sanitize-object-deep-by-regex",
  "version": "1.0.0",
  "description": "",
  "repository": "github:KATT/sanitize-object-deep-by-regex",
  "main": "dist/index.js",
  "files": [
    "dist/index.d.ts",
    "dist/index.js.map",
    "dist/index.js"
  ],
  "scripts": {
    "build": "rimraf dist && tsc",
    "test": "jest",
    "benchmark": "npm t && npm run build && node benchmark.js",
    "benchmark:watch": "nodemon --watch src -e ts --exec npm run benchmark",
    "perf:init": "rm -f src/alt.* && cp src/index.ts src/alt.ts && cp src/index.spec.ts src/alt.spec.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}!api/src/generated/*.ts": [
      "prettier --parser typescript --write",
      "tslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/**.d.ts"
    ],
    "testMatch": [
      "**/*.spec.(ts|tsx|js|jsx)"
    ]
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/benchmark": "2.1.2",
    "@types/jest": "27.5.2",
    "@types/node": "18.11.18",
    "benchmark": "2.1.4",
    "husky": "8.0.3",
    "jest": "27.5.1",
    "lint-staged": "13.1.0",
    "nodemon": "2.0.20",
    "prettier": "2.8.3",
    "rimraf": "4.0.5",
    "tslint": "6.1.3",
    "typescript": "4.9.4"
  },
  "dependencies": {}
}
